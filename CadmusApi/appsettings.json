{
  "Logging": {
    "IncludeScopes": false,
    "Debug": {
      "LogLevel": {
        "Default": "Warning"
      }
    },
    "Console": {
      "LogLevel": {
        "Default": "Warning"
      }
    }
  },
  "Data": {
    "DefaultConnection": {
      "ConnectionString": "Server=(local)\\SqlExpress;Database=cadmus;Trusted_Connection=True;MultipleActiveResultSets=true;"
    }
  },
  "Mongo": {
    "Template": "mongodb://localhost:27017/{0}"
  },
  "Serilog": {
    "ConnectionString": "Server=(local)\\SqlExpress;Database=cadmus;Trusted_Connection=True;MultipleActiveResultSets=true",
    "TableName": "Logs",
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  }
}
